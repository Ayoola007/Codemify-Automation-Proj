// No 8

function checkEquality(value1, value2) {
    return value1 === value2;
}

console.log(checkEquality(1, true)); 
console.log(checkEquality(0, "0"));
console.log(checkEquality(1, 1));

// No 9

function compareAge(name, age) {
    const myAge = 32; 
    if (age > myAge) {
        return `${name} is older than me.`;
    } else if (age < myAge) {
        return `${name} is younger than me.`;
    }
}

console.log(compareAge("Joel", 36));

function compareAge(name, age) {
    const myAge = 32; 
    if (age > myAge) {
        return `${name} is older than me.`;
    } else if (age < myAge) {
        return `${name} is younger than me.`;
    }
}

console.log(compareAge("Samuel", 24));

function compareAge(name, age) {
    const myAge = 32; 
    if (age > myAge) {
        return `${name} is older than me.`;
    } else if (age < myAge) {
        return `${name} is younger than me.`;
    }
}

console.log(compareAge("Lily", 28));

