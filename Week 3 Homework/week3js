  function findLongerString(firstString, secondString) {
    if(firstString.length > secondString.length){
      return firstString
    }
    else{
      return secondString
    }
  }
  console.log (findLongerString("Codemify", "test"))

  function findLongerString(firstString, secondString) {
    if(firstString.length > secondString.length){
      return firstString
    }
    else{
      return secondString
    }
  }
  console.log (findLongerString("automation", "coding"))

